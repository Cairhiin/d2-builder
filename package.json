{
  "name": "d2-builder",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "cd server && nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "cd client/src && node app.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "nodemon": "^2.0.7"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
